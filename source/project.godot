[application]

config/name="Kitchen Tales"
run/main_scene="res://screens/splash_screen/SplashScreen.tscn"
boot_splash/image="res://screens/splash_screen/splash_image.png"
config/icon="res://icon.png"

[autoload]

PlayerData="*res://actors/player/PlayerData.tscn"
BGM="*res://screens/BGM.tscn"
Debug="*res://utility/Debug.tscn"

[debug]

gdscript/warnings/enable=false
gdscript/warnings/unused_variable=false
